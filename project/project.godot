config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/SQLBridge,org/godotengine/godot/GodotAds,org/godotengine/godot/AdMob,org/godotengine/godot/GooglePlay"

[application]

config/name="Clef Clicker"
run/main_scene="res://scenes/Root.tscn"
config/icon="res://sprites/appicon.png"

[autoload]

Debug="*res://scripts/singleton/Debug.gd"
Achievements="*res://scripts/singleton/Achievements.gd"
Leaderboards="*res://scripts/singleton/Leaderboards.gd"
PartitureFX="*res://scripts/singleton/PartitureFX.gd"
PlayServices="*res://scripts/singleton/PlayServices.gd"
Audio="*res://scenes/singleton/Audio.tscn"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=540
window/size/test_height=960
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[rendering]

